{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "gutenberg-parcel-example/gutenpride",
	"version": "0.1.0",
	"title": "Gutenpride Parcel",
	"category": "text",
	"icon": "hammer",
	"description": "A Gutenberg block to show your pride! This block enables you to type text and style it with the color font Gilbert from Type with Pride.",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h2",
			"placeholder": "Title"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"placeholder": "Content"
		}
	},
	"supports": {
		"html": false,
		"color": {
			"gradients": true
		}
	},
	"textdomain": "gutenberg-parcel-example",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./index.css"
}
